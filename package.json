{
  "name": "cook-me-ui",
  "version": "0.1.0",
  "private": true,
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.(js)": [
      "npm run lint:fix",
      "git add"
    ],
    "*.{js,css,scss,json,md}": [
      "npm run format",
      "git add"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.9.12",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.53",
    "axios": "^0.19.2",
    "final-form": "^4.19.1",
    "i18next": "^19.4.4",
    "i18next-http-backend": "^1.0.10",
    "lodash": "^4.17.15",
    "mui-datatables": "^2.14.0",
    "mui-rff": "^2.1.14",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-final-form": "^6.4.0",
    "react-i18next": "^11.4.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "commit": "git-cz",
    "format": "prettier --config .prettierrc --write './**/*.js'",
    "lint": "eslint .",
    "lint:debug": "npm run lint --debug",
    "lint:fix": "npm run lint --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "commitizen": "^4.0.5",
    "cz-conventional-changelog": "^3.1.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-compat": "^3.5.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "prop-types": "^15.7.2"
  }
}
